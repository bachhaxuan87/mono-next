{
  "version": 2,
  "env": {

  },
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/apps/backend/main.js",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "DELETE"
          ]
      }
  ]
}
