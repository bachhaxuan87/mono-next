{
  "root": "apps/bepay-mono",
  "sourceRoot": "apps/bepay-mono",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/bepay-mono",
        "outputPath": "dist/apps/bepay-mono"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "bepay-mono:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "bepay-mono:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "bepay-mono:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/bepay-mono"],
      "options": {
        "jestConfig": "apps/bepay-mono/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/bepay-mono/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
